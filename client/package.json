{
  "name": "test-the-web-client",
  "version": "0.0.0",
  "scripts": {
    "axe": "axe",
    "build:watch": "nodemon -x 'npm run build'",
    "build": "rimraf -rf build && docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "lint:a11y": "NODE_EXTRA_CA_CERTS=../api/ssl.crt node ./scripts/lint-a11y.js",
    "serve": "docusaurus serve --no-open",
    "start": "docusaurus start --no-open",
    "swizzle": "docusaurus swizzle",
    "typecheck": "tsc",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "dependencies": {
    "@docusaurus/core": "3.3.2",
    "@docusaurus/preset-classic": "3.3.2",
    "@material/web": "1.4.1",
    "@mdx-js/react": "3.0.1",
    "clsx": "2.1.1",
    "prism-react-renderer": "2.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@axe-core/cli": "4.9.0",
    "@docusaurus/module-type-aliases": "3.3.2",
    "@docusaurus/tsconfig": "3.3.2",
    "@docusaurus/types": "3.3.2",
    "@emotion/babel-plugin": "11.11.0",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@types/react": "18.3.1",
    "mui-one-time-password-input": "2.0.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "**/node_modules/**",
      "**/build/**",
      "**/.docusaurus/**"
    ],
    "watch": [
      "src/**",
      "docs/**",
      "pages/**",
      "static/**",
      "docusaurus.config.ts"
    ],
    "verbose": true,
    "ext": "js,jsx,json,ts,tsx,md,mdx,css"
  }
}
